<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/SubjectHasTagList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{subjectHasTagHome.wire}"/>

   <param name="subjectHasTagFrom"/>
   <param name="subjectHasTagSubject" value="#{subjectHasTagHome.subjectHasTagId.subject}"/>
   <param name="subjectHasTagTag" value="#{subjectHasTagHome.subjectHasTagId.tag}"/>
   <param name="subjectFrom"/>
   <param name="subjectId" value="#{subjectHome.subjectId}"/>

   <param name="tagsubjectFrom"/>
   <param name="tagsubjectId" value="#{tagsubjectHome.tagsubjectId}"/>



   <navigation from-action="#{subjectHasTagHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/SubjectHasTag.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{subjectHasTagHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/SubjectHasTag.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{subjectHasTagHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/SubjectHasTagList.xhtml"/>
      </rule>
   </navigation>

</page>
