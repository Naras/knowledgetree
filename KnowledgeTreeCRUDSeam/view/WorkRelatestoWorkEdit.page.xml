<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/WorkRelatestoWorkList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{workRelatestoWorkHome.wire}"/>

   <param name="workRelatestoWorkFrom"/>
   <param name="workRelatestoWorkWork1" value="#{workRelatestoWorkHome.workRelatestoWorkId.work1}"/>
   <param name="workRelatestoWorkWork2" value="#{workRelatestoWorkHome.workRelatestoWorkId.work2}"/>
   <param name="workFrom"/>
   <param name="workId" value="#{workHome.workId}"/>

   <param name="workWorkRelationFrom"/>
   <param name="workWorkRelationId" value="#{workWorkRelationHome.workWorkRelationId}"/>



   <navigation from-action="#{workRelatestoWorkHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/WorkRelatestoWork.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{workRelatestoWorkHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/WorkRelatestoWork.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{workRelatestoWorkHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/WorkRelatestoWorkList.xhtml"/>
      </rule>
   </navigation>

</page>
