<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/SubjectSubjectRelationList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{subjectSubjectRelationHome.wire}"/>

   <param name="subjectSubjectRelationFrom"/>
   <param name="subjectSubjectRelationId" value="#{subjectSubjectRelationHome.subjectSubjectRelationId}"/>


   <navigation from-action="#{subjectSubjectRelationHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/SubjectSubjectRelation.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{subjectSubjectRelationHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/SubjectSubjectRelation.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{subjectSubjectRelationHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/SubjectSubjectRelationList.xhtml"/>
      </rule>
   </navigation>

</page>
