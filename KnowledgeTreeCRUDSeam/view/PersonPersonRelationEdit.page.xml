<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/PersonPersonRelationList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{personPersonRelationHome.wire}"/>

   <param name="personPersonRelationFrom"/>
   <param name="personPersonRelationId" value="#{personPersonRelationHome.personPersonRelationId}"/>


   <navigation from-action="#{personPersonRelationHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/PersonPersonRelation.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{personPersonRelationHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/PersonPersonRelation.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{personPersonRelationHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/PersonPersonRelationList.xhtml"/>
      </rule>
   </navigation>

</page>
