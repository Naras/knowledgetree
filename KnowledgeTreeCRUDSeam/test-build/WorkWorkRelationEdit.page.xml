<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/WorkWorkRelationList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{workWorkRelationHome.wire}"/>

   <param name="workWorkRelationFrom"/>
   <param name="workWorkRelationId" value="#{workWorkRelationHome.workWorkRelationId}"/>


   <navigation from-action="#{workWorkRelationHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/WorkWorkRelation.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{workWorkRelationHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/WorkWorkRelation.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{workWorkRelationHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/WorkWorkRelationList.xhtml"/>
      </rule>
   </navigation>

</page>
