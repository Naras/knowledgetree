<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/ViewSubjectSubjectList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{viewSubjectSubjectHome.wire}"/>

   <param name="viewSubjectSubjectFrom"/>
   <param name="viewSubjectSubjectRelation" value="#{viewSubjectSubjectHome.viewSubjectSubjectId.relation}"/>
   <param name="viewSubjectSubjectRelationDescription" value="#{viewSubjectSubjectHome.viewSubjectSubjectId.relationDescription}"/>
   <param name="viewSubjectSubjectSub1id" value="#{viewSubjectSubjectHome.viewSubjectSubjectId.sub1id}"/>
   <param name="viewSubjectSubjectSub2id" value="#{viewSubjectSubjectHome.viewSubjectSubjectId.sub2id}"/>
   <param name="viewSubjectSubjectSubject1" value="#{viewSubjectSubjectHome.viewSubjectSubjectId.subject1}"/>
   <param name="viewSubjectSubjectSubject1description" value="#{viewSubjectSubjectHome.viewSubjectSubjectId.subject1description}"/>
   <param name="viewSubjectSubjectSubject2" value="#{viewSubjectSubjectHome.viewSubjectSubjectId.subject2}"/>
   <param name="viewSubjectSubjectSubject2description" value="#{viewSubjectSubjectHome.viewSubjectSubjectId.subject2description}"/>


   <navigation from-action="#{viewSubjectSubjectHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/ViewSubjectSubject.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{viewSubjectSubjectHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/ViewSubjectSubject.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{viewSubjectSubjectHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/ViewSubjectSubjectList.xhtml"/>
      </rule>
   </navigation>

</page>
