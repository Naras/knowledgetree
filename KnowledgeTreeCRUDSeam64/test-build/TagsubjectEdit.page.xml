<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/TagsubjectList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{tagsubjectHome.wire}"/>

   <param name="tagsubjectFrom"/>
   <param name="tagsubjectId" value="#{tagsubjectHome.tagsubjectId}"/>


   <navigation from-action="#{tagsubjectHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/Tagsubject.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{tagsubjectHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/Tagsubject.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{tagsubjectHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/TagsubjectList.xhtml"/>
      </rule>
   </navigation>

</page>
