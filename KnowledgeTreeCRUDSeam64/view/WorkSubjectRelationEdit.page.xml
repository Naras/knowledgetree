<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/WorkSubjectRelationList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{workSubjectRelationHome.wire}"/>

   <param name="workSubjectRelationFrom"/>
   <param name="workSubjectRelationId" value="#{workSubjectRelationHome.workSubjectRelationId}"/>


   <navigation from-action="#{workSubjectRelationHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/WorkSubjectRelation.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{workSubjectRelationHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/WorkSubjectRelation.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{workSubjectRelationHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/WorkSubjectRelationList.xhtml"/>
      </rule>
   </navigation>

</page>
