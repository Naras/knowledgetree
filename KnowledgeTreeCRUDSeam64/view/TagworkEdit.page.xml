<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/TagworkList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{tagworkHome.wire}"/>

   <param name="tagworkFrom"/>
   <param name="tagworkId" value="#{tagworkHome.tagworkId}"/>


   <navigation from-action="#{tagworkHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/Tagwork.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{tagworkHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/Tagwork.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{tagworkHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/TagworkList.xhtml"/>
      </rule>
   </navigation>

</page>
