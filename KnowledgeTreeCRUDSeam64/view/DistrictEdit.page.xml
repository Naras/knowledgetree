<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/DistrictList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{districtHome.wire}"/>

   <param name="districtFrom"/>
   <param name="districtCountryId" value="#{districtHome.districtId.countryId}"/>
   <param name="districtId" value="#{districtHome.districtId.id}"/>
   <param name="districtStateId" value="#{districtHome.districtId.stateId}"/>
   <param name="stateFrom"/>
   <param name="stateCountryId" value="#{stateHome.stateId.countryId}"/>
   <param name="stateId" value="#{stateHome.stateId.id}"/>
   <param name="countryFrom"/>
   <param name="countryId" value="#{countryHome.countryId}"/>




   <navigation from-action="#{districtHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/District.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{districtHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/District.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{districtHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/DistrictList.xhtml"/>
      </rule>
   </navigation>

</page>
