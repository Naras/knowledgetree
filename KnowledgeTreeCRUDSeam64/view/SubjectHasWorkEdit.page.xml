<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/SubjectHasWorkList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{subjectHasWorkHome.wire}"/>

   <param name="subjectHasWorkFrom"/>
   <param name="subjectHasWorkSubject" value="#{subjectHasWorkHome.subjectHasWorkId.subject}"/>
   <param name="subjectHasWorkWork" value="#{subjectHasWorkHome.subjectHasWorkId.work}"/>
   <param name="subjectHasWorkWorkSubjectRelation" value="#{subjectHasWorkHome.subjectHasWorkId.workSubjectRelation}"/>
   <param name="subjectFrom"/>
   <param name="subjectId" value="#{subjectHome.subjectId}"/>

   <param name="workFrom"/>
   <param name="workId" value="#{workHome.workId}"/>

   <param name="workSubjectRelationFrom"/>
   <param name="workSubjectRelationId" value="#{workSubjectRelationHome.workSubjectRelationId}"/>



   <navigation from-action="#{subjectHasWorkHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/SubjectHasWork.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{subjectHasWorkHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/SubjectHasWork.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{subjectHasWorkHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/SubjectHasWorkList.xhtml"/>
      </rule>
   </navigation>

</page>
