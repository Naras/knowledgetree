<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/SubjectRelatestoSubjectList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{subjectRelatestoSubjectHome.wire}"/>

   <param name="subjectRelatestoSubjectFrom"/>
   <param name="subjectRelatestoSubjectSubject1" value="#{subjectRelatestoSubjectHome.subjectRelatestoSubjectId.subject1}"/>
   <param name="subjectRelatestoSubjectSubject2" value="#{subjectRelatestoSubjectHome.subjectRelatestoSubjectId.subject2}"/>
   <param name="subjectFrom"/>
   <param name="subjectId" value="#{subjectHome.subjectId}"/>

   <param name="subjectSubjectRelationFrom"/>
   <param name="subjectSubjectRelationId" value="#{subjectSubjectRelationHome.subjectSubjectRelationId}"/>



   <navigation from-action="#{subjectRelatestoSubjectHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/SubjectRelatestoSubject.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{subjectRelatestoSubjectHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/SubjectRelatestoSubject.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{subjectRelatestoSubjectHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/SubjectRelatestoSubjectList.xhtml"/>
      </rule>
   </navigation>

</page>
