<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/PersonHasAffiliationList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{personHasAffiliationHome.wire}"/>

   <param name="personHasAffiliationFrom"/>
   <param name="personHasAffiliationAffiliation" value="#{personHasAffiliationHome.personHasAffiliationId.affiliation}"/>
   <param name="personHasAffiliationPerson" value="#{personHasAffiliationHome.personHasAffiliationId.person}"/>
   <param name="affiliationFrom"/>
   <param name="affiliationId" value="#{affiliationHome.affiliationId}"/>

   <param name="personFrom"/>
   <param name="personId" value="#{personHome.personId}"/>
   <param name="personlifeFrom"/>
   <param name="personlifeId" value="#{personlifeHome.personlifeId}"/>




   <navigation from-action="#{personHasAffiliationHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/PersonHasAffiliation.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{personHasAffiliationHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/PersonHasAffiliation.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{personHasAffiliationHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/PersonHasAffiliationList.xhtml"/>
      </rule>
   </navigation>

</page>
