<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/PersonHasRoleList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{personHasRoleHome.wire}"/>

   <param name="personHasRoleFrom"/>
   <param name="personHasRolePerson" value="#{personHasRoleHome.personHasRoleId.person}"/>
   <param name="personHasRoleRole" value="#{personHasRoleHome.personHasRoleId.role}"/>
   <param name="personFrom"/>
   <param name="personId" value="#{personHome.personId}"/>
   <param name="personlifeFrom"/>
   <param name="personlifeId" value="#{personlifeHome.personlifeId}"/>


   <param name="roleFrom"/>
   <param name="roleId" value="#{roleHome.roleId}"/>



   <navigation from-action="#{personHasRoleHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/PersonHasRole.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{personHasRoleHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/PersonHasRole.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{personHasRoleHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/PersonHasRoleList.xhtml"/>
      </rule>
   </navigation>

</page>
